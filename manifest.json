{
    "manifest_version": 3,
    "name": "Font Detector",
    "description": "Detect fonts on a webpage",
    "version": "1.0.0",
    "permissions": ["scripting", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "Click to Detect Fonts"
    }
  }
  